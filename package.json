  "license": "MIT",
  "description": "Contextium Service",
  "scripts": {
    "build": "node build.js",
    "start": "node dist/app.js",
    "dev": "tsx watch src/app.ts",
    "test": "vitest --coverage",
    "test:watch": "vitest --watch",
