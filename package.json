    "format:fix": "npm run format:fix:eslint && npm run format:fix:prettier"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "pg": "^8.13.1",
